<script>
    var isTouch = function () {
       return ('ontouchstart' in window || navigator.msMaxTouchPoints) ? true : false
    }
    if (isTouch()) {
        $('li.addlightbox .add-to-lightbox').removeAttr('onclick');
        $('body').addClass('touch');
    } else {
        $('body').addClass('no-touch');
    }

    var LBLoginReq = ('False'.toLowerCase() == "true");
    var lightboxGlobalResourcesString = '{"SorryUnableToContactTheServerPleaseTryAfterSomeTime":"Sorry, Unable to contact the server, please try after some time","ALightboxWithSameNameAlreadyExist":"Please choose a unique name for your lightbox.","PleaseEnterNameForLightbox":"Please enter a lightbox name (no invalid characters)","PleaseFillTheMandatoryFields":"Please fill the mandatory fields"}';
    var lightboxResources = {};

    if (lightboxGlobalResourcesString != null) {
        try {
            lightboxGlobalResources = JSON.parse(lightboxGlobalResourcesString);
        }
        catch (ex) {
            console.warn("Unable to parse resource string.");
        }
    }
    var addItemToLightboxResourcesString = '{"ItemSavedSuccess":"Saved to @@LightboxName","ViewLightbox":"View lightbox"}';
    var addItemToLightboxResources = {};

    if (addItemToLightboxResourcesString != null) {
        try {
            addItemToLightboxResources = JSON.parse(addItemToLightboxResourcesString);
        }
        catch (ex) {
            console.warn("Unable to parse resource string.");
        }
    }

    var currentPage = 'ImageComp';

    function addItemToLightboxShowModal() {        
        var dialogTitle = "Save to lightbox";          
        $('body, footer').css('width', (window.innerWidth - (window.innerWidth - document.body.clientWidth)) + "px");
        $('#addItemDialogTitle').html('<strong>' + dialogTitle + '</strong>');

        if(currentPage== "ImageComp"){
            var src = $('#imgPopup').attr("src");   
            $('#imagepreview').attr("src",src);
        }
        else if (currentPage == "VideoComp")
        {
            //var src = "https://p1.alamy.com/video/" + FootageID + "/" + FootageRef + ".jpg";   
            //$('#imagepreview').attr("src", src);
            $('#imagepreview').hide();
            var videoHTML = '<video controls="" style="outline: 0;width:385px; height:227px;border-radius: 4px; display:block" controlslist="nodownload" muted="" autoplay="" src="' + $('#videos_player').attr('src') + '"></video>';
            $('.lb-img-preview-clip').append(videoHTML);
        }

        $("#txtNewLightboxAddItem").val('');
        $(".in-scroll-list").show();
        $("#frmAddItemToLightbox").find("#createnewlb").hide();
        LoadLightboxNameListAddItem();

        addItemToLightboxSetBootstapValidation();
        $('#frmAddItemToLightbox').bootstrapValidator('resetForm', true);
        $('#btnConfirm_AddItemToLightbox').addClass('disabled');
        $('.copy-lightbox-modal').modal('show');

        $('#frmAddItemToLightbox .new-form-control').each(function (index) {
            if ($(this).val() != '') {
                $(this).parent().find('label.new-float-label').addClass('filled');
            }
        });
    }

    function LoadLightboxNameListAddItem() {
        var loggedIn = isUserLoggedIn();
        //if (!loggedIn) {
        //    redirectToLogin(alamyUser.currentLightboxID);
        //    return;
        //}

        var list, frag = document.createDocumentFragment();
        var sourceLightboxId = alamyUser.currentLightboxID;
        $.ajax({
            type: "POST",
            url: "/Lightbox/LightboxService.aspx/GetUserLightBoxes",
            data: "{LightboxID:'" + sourceLightboxId + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (result) {
                var lightBoxes = result.d.LBItems;
                var otherLBCount = 0;
                console.log('LBITems: ' + lightBoxes.length);
                for (idx = 0; idx < result.d.LBItems.length; idx++) {
                    //if (lightBoxes[idx].selected != "1") {
                    list = $("<li  onclick=addItemToLightbox('" + lightBoxes[idx].lightboxid + "')>" + unescape(lightBoxes[idx].lightboxname).replace(/</g, "&lt;").replace(/>/g, "&gt;") + "</li>");
                    frag.appendChild(list[0]);
                    otherLBCount++;
                    //}
                }
                if (otherLBCount <= 0) {
                    $("#frmAddItemToLightbox").find("#createnewlb").show();
                    $(".in-scroll-list").hide();
                }
                $("#ulLightboxesAddItem").html(frag);
            },
            error: function (result) {
                console.log(lightboxGlobalResources["SorryUnableToContactTheServerPleaseTryAfterSomeTime"]);
                //alert(lightboxGlobalResources["SorryUnableToContactTheServerPleaseTryAfterSomeTime"]);
            }
        });
    }

    function addItemToLightboxSetBootstapValidation() {
        $('#frmAddItemToLightbox').bootstrapValidator({
            message: lightboxGlobalResources["PleaseFillTheMandatoryFields"],
            excluded: [':disabled', ':hidden', ':not(:visible)'],
            submitHandler: function (validator, form, submitButton) {
                addItemToNewLB();
            },
            fields: {
                txtNewLightboxAddItem: {
                    trigger: 'blur',
                    validators: {
                        notEmpty: {
                            message: lightboxGlobalResources["PleaseEnterNameForLightbox"]
                        },
                        regexp: {
                            regexp: /^[^*|\":^~<>[\]{}`\\()';@$]+$/i,
                            message: lightboxGlobalResources["PleaseEnterNameForLightbox"]
                        },
                    }
                },
            }
        })
    }

    var addItemToLightboxIconClick = function (lightboxid) {
        if (LBLoginReq)
            redirectToLogin($('#hidCurrentLightboxId').val());
        else {
            if ($('#hidCurrentLightboxId').val() != '') {
                addItemToLightbox();
            }
        }
    }

    var addItemToLightbox = function (lightboxid) {
        var targetLightboxId;
        if (lightboxid==undefined)
            targetLightboxId = $('#hidCurrentLightboxId').val();
        else
            targetLightboxId = lightboxid;
        var loggedIn = isUserLoggedIn();
        //if (!loggedIn) {
        //    redirectToLogin(alamyUser.currentLightboxID);
        //    return;
        //}

        if (targetLightboxId == '') {
            //user logged in, but do not have any active lightboxes
            addItemToLightboxShowModal();
            return;
        };

        $('.lb-save-list').fadeOut();
        if (currentPage == "ImageComp") {
            objGTM.setValues("Add", "Add to Lightbox", "");            

          //  if (loggedIn == true) {
                var collectionid = '', noveluse = false;
                var params = '{"lbid": "' + targetLightboxId + '"' +
                             ',"itemid": "' + imageHeader.imageId + '"' +
                             ',"itemtype":' + imageHeader.imageType +
                             ',"itemref": "' + imageHeader.imageRef + '"' +
                             ',"lictype": ' + imageHeader.licenseType +
                             ',"collectionid":"' + collectionid + '"' +
                             ', "noveluse": ' + noveluse + '}';
                $.ajax({
                    type: "POST",
                    url: "/Lightbox/LightboxService.aspx/AddItemToLightbox",
                    data: params,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: addItemToLightboxOnSuccess,
                    error: addItemToLightboxOnFailure,
                });
           // }            
        }
        else if (currentPage == "VideoComp") {
            objGTM.setValues("Video Icon Clicks", "Add to Lightbox", "");
            //Set the selected LB as active lightbox
            //if (loggedIn == true) {
                var collectionid = '', noveluse = false;
                var params = '{"lbid": "' + targetLightboxId + '"' +
                             ',"itemid": "' + videoHeader.videoId + '"' +                             
                             ',"itemref": "' + FootageRef + '"' +
                             ',"lictype": 0}';
                $.ajax({
                    type: "POST",
                    url: "/Lightbox/LightboxService.aspx/AddVideoToLightbox",
                    data: params,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: addItemToLightboxOnSuccess,
                    error: addItemToLightboxOnFailure,
                });
          //  }
        }
    }

    function addItemToLightboxOnSuccess(result) {
        var currentLightbox = result.d;
        if (currentLightbox.response.status == true) {
            //overwrite global vars
            alamyUser.currentLightboxID = currentLightbox.lightboxid;
            alamyUser.currentLightboxName = currentLightbox.lightboxname;
            //hide the modal
            $('.copy-lightbox-modal').modal('hide');
            //display success message            
            var msg = addItemToLightboxResources["ItemSavedSuccess"];
            msg = msg.replace("@@LightboxName", "<b>" + alamyUser.currentLightboxName + "</b>");
            var viewLightbox = addItemToLightboxResources["ViewLightbox"];            
            //show message at the bottom                      
            createInfoTooltip(msg, { Text: viewLightbox, Url: '/' + requestHeader.urlPart + 'lightbox/details.aspx?lbid=' + alamyUser.currentLightboxID });
            $('#hidCurrentLightboxName').val(alamyUser.currentLightboxName);
        }
        else {
            $('.copy-lightbox-modal').modal('hide');
        }
    }

    function addItemToLightboxOnFailure() {
        console.log(lightboxGlobalResources["SorryUnableToContactTheServerPleaseTryAfterSomeTime"]);
        //alert(lightboxGlobalResources["SorryUnableToContactTheServerPleaseTryAfterSomeTime"]);
    }

    var addItemToNewLB = function () {
        var lightboxName = $("#txtNewLightboxAddItem").val();
        var langcode = 'en';        
        if (currentPage == "ImageComp") {
            var collectionid = '', noveluse = false;
            var params = '{"lightboxname": "' + lightboxName + '"' +
                         ',"langcode": "' + langcode + '"' +
                         ',"itemid": "' + imageHeader.imageId + '"' +
                         ',"itemtype":' + imageHeader.imageType +
                         ',"itemref": "' + imageHeader.imageRef + '"' +
                         ',"lictype": ' + imageHeader.licenseType +
                         ',"collectionid":"' + collectionid + '"' +
                         ', "noveluse": ' + noveluse + '}';
            $.ajax({
                type: "POST",
                url: "/Lightbox/LightboxService.aspx/AddItemToNewLightbox",
                data: params,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: addItemToLightboxOnSuccess,
                error: addItemToLightboxOnFailure,
            });
        }
        else if (currentPage == "VideoComp") {
            var params = '{"lightboxname": "' + lightboxName + '"' +
                         ',"langcode": "' + langcode + '"' +
                         ',"itemid": "' + videoHeader.videoId + '"' +                         
                         ',"itemref": "' + FootageRef + '"' +
                         ',"lictype": 0}';
            $.ajax({
                type: "POST",
                url: "/Lightbox/LightboxService.aspx/AddVideoToNewLightbox",
                data: params,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: addItemToLightboxOnSuccess,
                error: addItemToLightboxOnFailure,
            });
        }
    }    

    var redirectToLogin = function (lightboxId) {
        try {
            if(currentPage=="ImageComp")
            {
                var retURL = window.location.href.split("?")[0] + "?cmd=add2lb";
                retURL = retURL + "&origin=id";
                retURL = addURLParam(retURL, "imageid", imageHeader.imageId);
                retURL = addURLParam(retURL, "clbid", lightboxId);
                retURL = addURLParam(retURL, "ref", imageHeader.imageRef);
                retURL = addURLParam(retURL, "lictype", imageHeader.licenseType);
                //retURL += cname;
                retURL = escape(retURL);                
                window.location.href = "/log-in/?returnurl=" + retURL;
            }
            else if(currentPage=="VideoComp")
            {
                var retURL = window.location.href.split("?")[0] + "?cmd=addFootage2lb";
                retURL = retURL + "&origin=id";
                retURL = addURLParam(retURL, "VideoID", videoHeader.videoId);                
                retURL = addURLParam(retURL, "ref", FootageRef);                                
                retURL = escape(retURL);                
                window.location.href = "/log-in/?returnurl=" + retURL;
            }
        } catch (err) {
        }
    }

    var isUserLoggedIn = function () {
        var result = true;
        var userId = GetCookie("AlamyUserID");
        if (userId == null || userId == "undefined" || userId == "") {
            result = false;
        }
        return result;
    }
    function copyModalShowMessage(message) {
        $('.copy-lightbox-modal').find('#error-div').html(message);
        $('.copy-lightbox-modal').find('#error-div').css("display", "block");
        setTimeout('shareModalClearMessage()', 4000);
    }

    function copyModalClearMessage() {
        $('.copy-lightbox-modal').find('#error-div').html('');
        $('.copy-lightbox-modal').find('#error-div').css("display", "none");
    }
    
    var addURLParam = function (sURL, sName, sValue) {
    if (sURL.toLowerCase().indexOf(sName.toLowerCase()) == -1)
        return sURL + "&" + sName + "=" + sValue

    return sURL
    };

    /* Lightbox info box tooltip*/
    $('.lb-save-list').fadeOut('fast');
    var tOut, tsOut;
    var triggerTooltip = function (el) {
        if (!LBLoginReq) {
            clearTimeout(tsOut);
            if ((el.offset().top - $(window).scrollTop() - 10) < $('.lb-save-list').outerHeight())
                $('.lb-save-list').css('top', (el.offset().top + el.outerHeight() + 5) - $(window).scrollTop()).removeClass('top');
            else
                $('.lb-save-list').css('top', ((el.offset().top - $(window).scrollTop()) - $('.lb-save-list').outerHeight() - 15)).addClass('top');

            if ((el.offset().left - 10) + $('.lb-save-list').outerWidth() >= $(window).width()) {
                $('.lb-save-list').css('left', $(window).width() - ($('.lb-save-list').outerWidth() + 25));
                $('.lb-save-list .filled-arrow-down').css('left', (el.offset().left - parseInt($('.lb-save-list').css('left')) + 9));
            } else {
                $('.lb-save-list').css('left', el.offset().left - 16); 
                $('.lb-save-list .filled-arrow-down').css('left', '24px');
            }
            var lightboxName = ($('#hidCurrentLightboxName').val().length > 0) ? $('#hidCurrentLightboxName').val() : 'Create new lightbox';
            //$('.lb-save-list').css({ 'top': ($(this).find('.icon-lightbox-n').offset().top - ($(window).scrollTop() + $('.lb-save-list').outerHeight() + 15)), 'left': $(this).find('.icon-lightbox-n').offset().left - 10 });
            $('.lb-save-list').find('#divCurrentLB').html(lightboxName);
            $('.lb-save-list').fadeIn('medium');
        }
    }
    $('li.addlightbox .add-to-lightbox').click(function () {
        if (isTouch()) {
            $(this).find('i').removeAttr('onclick');
            triggerTooltip($(this));
        }
    });
    $(document).click(function (e) {
        if(!$(e.target).hasClass('add-to-lightbox'))
            $('.lb-save-list').hide();
    });
    $('li.addlightbox .add-to-lightbox').hover(function () {
        if (!isTouch())
            triggerTooltip($(this));
    }, 
    function () {
        tOut = setTimeout(function () {
            $('.lb-save-list').hide();
        }, 4000);
    });

    $('.lb-save-list').hover(function () {
        clearTimeout(tOut);
    },
    function(){
        tsOut = setTimeout(function () {
            if (!IsMobiledevice)
                $('.lb-save-list').hide();
        }, 4000);    
    });

    $(window).scroll(function () {
        $('.lb-save-list').fadeOut();
    });
    $('body').on('mouseup', function (e) {
        if (!IsMobiledevice) {
            var container = $('.lb-save-list');
            if (!container.is(e.target)
                && container.has(e.target).length === 0) {
                container.fadeOut();
            }
        }
    });
    //For info tooltip
    var createInfoTooltip = function (data, link) {
        //$('.info-tooltip').remove();
        var lnkHtml = ((link != undefined) ? '<a class="ml-10" href="' + link.Url + '">' + link.Text + '</a>' : '');
        $('.info-tooltip span').html(data + lnkHtml);
        //$('.info-tooltip').addClass('show');
        setTimeout(function () {
            $('.info-tooltip').addClass('show');
        }, 1000);
        setTimeout(function () {
            $('.info-tooltip').removeClass('show');
        }, 5000);
    };

    $('#txtNewLightboxAddItem').keyup(function () {
        if ($(this).val().length > 0)
            $(this).parent().find('.in-scroll-btn').removeClass('disabled');
        else
            $(this).parent().find('.in-scroll-btn').addClass('disabled');
    });
    $('#txtNewLightboxAddItem').blur(function () {
        if ($(this).val().length > 0) 
            $(this).next('.new-float-label').addClass('filled');
        else 
            $(this).next('.new-float-label').addClass('filled');
    });
</script>
